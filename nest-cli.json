{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "root": "apps/main-panel",
  "sourceRoot": "apps/main-panel/src",
  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "apps/main-panel/tsconfig.app.json",
    "deleteOutDir": true
  },
  "projects": {
    "main-panel": {
      "type": "application",
      "root": "apps/main-panel",
      "entryFile": "main",
      "sourceRoot": "apps/main-panel/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main-panel/tsconfig.app.json"
      }
    },
    "admin-panel": {
      "type": "application",
      "root": "apps/admin-panel",
      "entryFile": "main",
      "sourceRoot": "apps/admin-panel/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-panel/tsconfig.app.json"
      }
    },
    "mobile-api": {
      "type": "application",
      "root": "apps/mobile-api",
      "entryFile": "main",
      "sourceRoot": "apps/mobile-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mobile-api/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "content": {
      "type": "library",
      "root": "libs/content",
      "entryFile": "index",
      "sourceRoot": "libs/content/src",
      "compilerOptions": {
        "tsConfigPath": "libs/content/tsconfig.lib.json"
      }
    },
    "billing": {
      "type": "library",
      "root": "libs/billing",
      "entryFile": "index",
      "sourceRoot": "libs/billing/src",
      "compilerOptions": {
        "tsConfigPath": "libs/billing/tsconfig.lib.json"
      }
    },
    "customer": {
      "type": "library",
      "root": "libs/customer",
      "entryFile": "index",
      "sourceRoot": "libs/customer/src",
      "compilerOptions": {
        "tsConfigPath": "libs/customer/tsconfig.lib.json"
      }
    }
  }
}

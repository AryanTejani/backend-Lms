{
  "name": "traderlion-platform-backend",
  "version": "1.0.0",
  "description": "TraderLion Platform Backend Monorepo",
  "main": "dist/apps/main-panel/apps/main-panel/src/main.js",
  "scripts": {
    "dev": "nest start main-panel --watch",
    "dev:admin": "nest start admin-panel --watch",
    "dev:mobile": "nest start mobile-api --watch",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:admin\" \"npm run dev:mobile\"",
    "build": "nest build main-panel",
    "build:admin": "nest build admin-panel",
    "build:mobile": "nest build mobile-api",
    "build:all": "nest build main-panel && nest build admin-panel && nest build mobile-api",
    "start": "node dist/apps/main-panel/apps/main-panel/src/main.js",
    "start:admin": "node dist/apps/admin-panel/apps/admin-panel/src/main.js",
    "start:mobile": "node dist/apps/mobile-api/apps/mobile-api/src/main.js",
    "start:prod": "NODE_ENV=production node dist/apps/main-panel/apps/main-panel/src/main.js",
    "start:prod:admin": "NODE_ENV=production node dist/apps/admin-panel/apps/admin-panel/src/main.js",
    "start:prod:mobile": "NODE_ENV=production node dist/apps/mobile-api/apps/mobile-api/src/main.js",
    "migrate": "node-pg-migrate up -m migrations",
    "migrate:down": "node-pg-migrate down -m migrations",
    "migrate:create": "node-pg-migrate create -m migrations",
    "lint": "eslint apps/ libs/",
    "lint:fix": "eslint apps/ libs/ --fix",
    "typecheck": "tsc --noEmit -p apps/main-panel/tsconfig.app.json && tsc --noEmit -p apps/admin-panel/tsconfig.app.json && tsc --noEmit -p apps/mobile-api/tsconfig.app.json",
    "seed:admin": "ts-node -r tsconfig-paths/register scripts/seed-admin.ts",
    "seed:plans": "ts-node -r tsconfig-paths/register scripts/seed-subscription-plans.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-aryan-deepvue:deepvue-app/traderlion-platform-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aws-sdk/client-sts": "^3.975.0",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/ioredis": "^4.28.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^25.0.10",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "concurrently": "^9.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^17.1.0",
    "nodemon": "^3.1.11",
    "prisma": "^7.3.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.750.0",
    "@google/genai": "^1.43.0",
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.12",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.5",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.9.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "edge-tts-universal": "^1.4.0",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "multer": "^2.0.2",
    "node-pg-migrate": "^8.0.4",
    "pg": "^8.17.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "stripe": "^20.3.1",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.3.6"
  }
}
